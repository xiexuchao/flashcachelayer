# system topology
topology disksim_iodriver driver0 [
   disksim_bus bus_dr2c [ 
      disksim_ctlr ctlr0 [  
	disksim_bus bus_c2di [ 
            disksim_disk disk0 []
         ], # end of bus_c2di
      	# anjo
         disksim_bus bus_c2f [
         	ssdmodel_ssd ssd0 []
         ] # end of bus_c2f
         # anjo
      ] # end of ctlr0
   ] # end of bus_dr2c
] # end of system topology 

